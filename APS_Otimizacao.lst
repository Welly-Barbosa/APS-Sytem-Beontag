 588  * ------------------------------------------------------------------------------
 589  * 1.2 Declaracao de Conjuntos e Parametros Adicionais
 590  * ------------------------------------------------------------------------------
 591  Sets
 592      pt       'Universo de padroes de corte potenciais' /pt_dummy, Job001*Job999/;
 593  Alias (t, tt, t2, t_prod);
 594  Alias (pt, pt_new);
 595   
 596  * --- Parametros para calculo da capacidade ---
 597  Parameter p_velocidadeEfetiva(j) 'pol/min efetivo';
 598  p_velocidadeEfetiva(j) = p_velocidadeMaq(j) * p_eficiencia(j);
 599   
 600  Parameter p_tempoProcesso(j) 'min por bobina-mae';
 601  p_tempoProcesso(j)$p_velocidadeEfetiva(j) = s_comprimentoMae_pes / p_velocidadeEfetiva(j);
 602  Parameter p_tempoUsoMaquina(j) 'min por uso na maquina j (setup + processo)';
 603  p_tempoUsoMaquina(j) = p_tempoSetupBase(j) + p_tempoProcesso(j);
 604  * --- Parametros de Custo e Qualidade ---
 605  Scalar s_pesoCustoProducao   'Peso do custo de producao (por bobina-mae usada)' / 1 /;
 606  Scalar W_backlog             'Peso gigante para penalizar o backlog no objetivo' /1e6/;
 607  Scalar s_pesoCustoRefugo     'Custo de refugo (sobra de largura), para desempate' / 0.1 /;
 608  Scalar s_minUtilPct_master   'Utilizacao minima para um padrao ser aceito pelo mestre' / 0.97 /;
 609   
 610  *-- ADICIONADO --*
 611  * --- Parametro para a restricao global de cortes (Premissa PoC) ---
 612  Scalar s_maxCortesGlobal     'Numero maximo de cortes permitido em um padrao';
 613  s_maxCortesGlobal = smin(j, p_maxCortes(j));
 614   
 615   
 616  Parameters
 617      p_precoDual(p_base,w,c)         'Preco dual da demanda (para pricing)'
 618      p_refugoLargura(pt)             'Largura nao utilizada (refugo) do padrao pt';
 619  * Mapeamento de dados dos produtos
 620  Parameters
 621      p_larguraPwC(p_base,w,c)    'Largura numerica do produto'
 622      p_compPwC(p_base,w,c)       'Comprimento numerico do produto';
 623  p_larguraPwC(p_base,w,c)$(p_larguraProduto(p_base,w,c)) = p_larguraProduto(p_base,w,c);
 624  p_compPwC(p_base,w,c)$(p_comprimentoProduto(p_base,w,c)) = p_comprimentoProduto(p_base,w,c);
 625   
 626  * ==============================================================================
 627  * FASE 2: DEFINICAO DO PROBLEMA MESTRE E SUBPROBLEMA
 628  * ==============================================================================
 629   
 630  Parameters
 631      p_rendimentoPadrao(pt,p_base,w,c) 'Qtd do produto (p,w,c) por padrao pt';
 632  Set pt_on(pt) 'Padroes ativos que podem ser usados pelo mestre';
 633  * --- Variaveis e Equacoes do Mestre ---
 634  Variables
 635      v_usaPadrao(pt, j, t)
 636      Bcum(p_base,w,c,t)
 637      v_custoProducao
 638      v_somaBacklog
 639      zTotal;
 640  Integer Variable v_usaPadrao;
 641  Positive Variable Bcum;
 642   
 643  Equations
 644      eq_capacidade(j, t)
 645      eq_backlogCum(p_base,w,c,t)
 646      eq_calcula_custo
 647      eq_calcula_backlog
 648      eq_obj_lex;
 649  *
 650  * Garante que o tempo total de uso dos padroes em uma maquina j no dia t nao exceda o tempo disponivel.
 651  eq_capacidade(j,t)$(p_tempoDisponivel(j,t) > 0)..
 652      sum(pt$pt_on(pt), v_usaPadrao(pt,j,t) * p_tempoUsoMaquina(j))
 653      =l= p_tempoDisponivel(j,t);
 654   
 655  *
 656  * Calcula o backlog (demanda nao atendida) cumulativo para cada produto ate cada dia t.
 657  eq_backlogCum(p_base,w,c,t)..
 658      Bcum(p_base,w,c,t) =g=
 659          sum(t2$(ord(t2) <= ord(t)), p_demanda(p_base,w,c,t2))
 660        - sum((pt,j,t2)$(ord(t2) <= ord(t)), p_rendimentoPadrao(pt,p_base,w,c) * v_usaPadrao(pt,j,t2));
 661   
 662  *
 663  * Calcula o custo total de producao, somando o custo por bobina-mae usada e uma penalidade pelo refugo de largura.
 664  eq_calcula_custo..
 665      v_custoProducao =e=
 666          s_pesoCustoProducao * sum((pt,j,t)$pt_on(pt), v_usaPadrao(pt,j,t))
 667        + s_pesoCustoRefugo   * sum((pt,j,t)$pt_on(pt), (p_refugoLargura(pt)/s_larguraMae) * v_usaPadrao(pt,j,t));
 668   
 669  *
 670  * Soma o backlog de todos os produtos em todos os periodos.
 671  eq_calcula_backlog.. v_somaBacklog =e= sum((p_base,w,c,t), Bcum(p_base,w,c,t));
 672   
 673  *
 674  * Funcao objetivo lexicografica: prioriza minimizar o backlog (com um peso muito alto) e, secundariamente, minimizar o custo de producao.
 675  eq_obj_lex.. zTotal =e= W_backlog * v_somaBacklog + v_custoProducao;
 676  * --- Modelo Mestre ---
 677  Model CorteMestre /
 678      eq_obj_lex, eq_calcula_custo, eq_calcula_backlog,
 679      eq_backlogCum, eq_capacidade
 680  /;
 681  * ------------------------------------------------------------------------------
 682  * --- Subproblema ---
 683  * ------------------------------------------------------------------------------
 684   
 685  Set c_on(c) 'comprimentos ativos no conjunto p';
 686  c_on(c) = yes$( sum((p_base,w)$p(p_base,w,c), 1) > 0 );
 687  Parameter UmaxCuts(p_base,w,c) 'limite sup. inteiro de cortes por produto no padrao';
 688  UmaxCuts(p_base,w,c) = 0;
 689  UmaxCuts(p_base,w,c)$(
 690      p(p_base,w,c) and p_larguraPwC(p_base,w,c) > 0 and
 691      p_larguraPwC(p_base,w,c) <= s_larguraMae
 692  ) = floor(s_larguraMae / p_larguraPwC(p_base,w,c));
 693  Scalar s_minUtilPct 'Target de utilizacao minima de largura por padrao' / 0.97 /;
 694   
 695  Variables v_valorPadrao;
 696  Integer Variable v_geraCorte(p_base,w,c);
 697  Binary Variable  y_len(c) '1 se o padrao escolhe o comprimento c';
 698  Equations
 699      eq_sub_objetivo, eq_sub_capacidade, eq_one_len,
 700      eq_link_len(p_base,w,c), eq_min_util,
 701      eq_sub_max_cortes;
 702   
 703  *
 704  * Funcao objetivo do subproblema: maximiza o valor do padrao, que e a soma dos precos duais dos produtos incluidos.
 705  eq_sub_objetivo..   v_valorPadrao =e= sum((p_base,w,c), p_precoDual(p_base,w,c) * v_geraCorte(p_base,w,c));
 706   
 707  *
 708  * Restricao de capacidade de largura: a soma das larguras dos produtos cortados nao pode exceder a largura da bobina-mae.
 709  eq_sub_capacidade.. sum((p_base,w,c), p_larguraPwC(p_base,w,c) * v_geraCorte(p_base,w,c)) =l= s_larguraMae;
 710   
 711  *
 712  * Garante que um padrao de corte pode ter produtos de apenas um unico comprimento de bobina-filha.
 713  eq_one_len..        sum(c$c_on(c), y_len(c)) =e= 1;
 714   
 715  *
 716  * Acopla a variavel de decisao de corte (v_geraCorte) a variavel de escolha de comprimento (y_len). So pode cortar se o comprimento for escolhido.
 717  eq_link_len(p_base,w,c)$p(p_base,w,c)..
 718      v_geraCorte(p_base,w,c) =l= UmaxCuts(p_base,w,c) * y_len(c);
 719   
 720  *
 721  * Garante que a utilizacao da largura da bobina-mae seja acima de um percentual minimo para evitar padroes com muito refugo.
 722  eq_min_util..
 723      sum((p_base,w,c)$p(p_base,w,c), p_larguraPwC(p_base,w,c) * v_geraCorte(p_base,w,c))
 724      =g= s_minUtilPct * s_larguraMae * sum(c$c_on(c), y_len(c));
 725   
 726  *-- ADICIONADO --*
 727  *
 728  * Restricao de numero maximo de cortes: a soma total de itens (cortes) no padrao nao pode exceder o limite minimo global das maquinas.
 729  eq_sub_max_cortes..
 730      sum((p_base,w,c), v_geraCorte(p_base,w,c)) =l= s_maxCortesGlobal;
 731   
 732   
 733  Model GeraPadrao /
 734      eq_sub_objetivo, eq_sub_capacidade, eq_one_len,
 735      eq_link_len, eq_min_util,
 736      eq_sub_max_cortes
 737  /;
 738  *-- ALTERADO --*
 739   
 740  v_geraCorte.lo(p_base,w,c) = 0;
 741  v_geraCorte.up(p_base,w,c) = UmaxCuts(p_base,w,c);
 742  * ==============================================================================
 743  * FASE 3: LOOP DE GERACAO DE COLUNAS
 744  * ==============================================================================
 745  Scalar
 746      s_contadorPadroes   / 0 /,   s_podeMelhorar  / 1 /,     s_iter / 0 /,
 747      s_iterMax           / 499 /, s_semMelhora / 0 /,     s_semMelhoraMax / 10 /,
 748      s_epsPreco          / 0.001 /, s_lastCusto / INF /,
 749      s_tempoMaximoTotal  / 300 /, s_timeStart;
 750  Scalar p_fillLargura_cand 'Ocupacao do padrao candidato para validacao';
 751   
 752  * --- NOVO: Inicializacao de um padrao "dummy" para a primeira iteracao ---
 753  * O padrao dummy permite que o primeiro SOLVE execute sem erro ($66).
 754  * Ele nao tem custo nem rendimento, sendo ignorado pela otimizacao.
 755  p_rendimentoPadrao('pt_dummy', p_base, w, c) = 0;
 756  p_refugoLargura('pt_dummy') = 0;
 757  pt_on('pt_dummy') = yes;
 758   
 759  * --- Loop Principal ---
 760  s_timeStart = jnow;
 761  WHILE( (s_podeMelhorar = 1) and (s_iter < s_iterMax) and (s_semMelhora < s_semMelhoraMax) and ((jnow - s_timeStart)*86400 < s_tempoMaximoTotal),
 762      s_iter = s_iter + 1;
 763   
 764      SOLVE CorteMestre using RMIP minimizing zTotal;
 765   
 766      p_precoDual(p_base,w,c) = sum(t, eq_backlogCum.m(p_base,w,c,t));
 767      put_utility 'log' / '[INFO] Iteracao ', s_iter:0:0, ' | Backlog: ', v_somaBacklog.l:0:4, ' | Custo: ', v_custoProducao.l:0:2;
 768   
 769      if( s_lastCusto < INF,
 770          if( abs(s_lastCusto - zTotal.l) < 1e-4, s_semMelhora = s_semMelhora + 1; else s_semMelhora = 0; );
 771      );
 772      s_lastCusto = zTotal.l;
 773   
 774      s_podeMelhorar = 0;
 775      SOLVE GeraPadrao using MIP maximizing v_valorPadrao;
 776   
 777      if (v_valorPadrao.l > 1 + s_epsPreco,
 778          put_utility 'log' / '[INFO] Novo padrao com valor: ', v_valorPadrao.l:0:4;
 779          s_podeMelhorar = 1;
 780          s_contadorPadroes = s_contadorPadroes + 1;
 781   
 782  * NOVO: Desativa o padrao dummy apos a criacao do primeiro padrao real
 783          if(s_contadorPadroes = 1,
 784              pt_on('pt_dummy') = no;
 785          );
 786   
 787          loop(pt_new$(ord(pt_new) = s_contadorPadroes + 1),
 788  * Logica de aceitacao com gatekeeper de qualidade para TODOS os padroes
 789             p_rendimentoPadrao(pt_new, p_base,w,c) = v_geraCorte.l(p_base,w,c);
 790             p_fillLargura_cand = sum((p_base,w,c), p_larguraPwC(p_base,w,c) * p_rendimentoPadrao(pt_new,p_base,w,c));
 791   
 792             if (p_fillLargura_cand >= s_minUtilPct_master * s_larguraMae,
 793                 pt_on(pt_new) = yes;
 794                 p_refugoLargura(pt_new) = s_larguraMae - p_fillLargura_cand;
 795                 put_utility 'log' / '[ACCEPT] Padrao ', pt_new.tl, ' aceito com utilizacao: ', ((p_fillLargura_cand/s_larguraMae)*100):0:2, '%';
 796             else
 797                 pt_on(pt_new) = no;
 798                 p_rendimentoPadrao(pt_new, p_base,w,c) = 0;
 799                 s_contadorPadroes = s_contadorPadroes - 1;
 800                 put_utility 'log' / '[REJECT] Padrao ', pt_new.tl, ' rejeitado com utilizacao: ', ((p_fillLargura_cand/s_larguraMae)*100):0:2, '%';
 801             );
 802          );
 803      );
 804  );
 805   
 806  * ==============================================================================
 807  * FASE 4: OTIMIZACAO FINAL E RELATORIOS
 808  * ==============================================================================
 809  if (CorteMestre.modelstat = 1 or CorteMestre.modelstat = 8,
 810      SOLVE CorteMestre using MIP minimizing zTotal;
 811  );
 812  * ------------------------------------------------------------------------------
 813  * 4.1 Relatorios de Saida em formato CSV
 814  * ------------------------------------------------------------------------------
 815  File
 816      f_composicao_csv    '/relatorio_composicao_padroes.csv/'
 817      f_plano_csv         '/relatorio_plano_producao.csv/'
 818      f_status_csv        '/relatorio_status_entregas.csv/';
 819  * --- Relatorio 1: Composicao dos Padroes Utilizados
 820  put f_composicao_csv 'PadraoCorte,PN_Base,LarguraProduto,CompProduto,QtdPorBobinaMae' /;
 821  loop((pt, p_base, w, c)$(p_rendimentoPadrao(pt, p_base, w, c) > 0),
 822      put f_composicao_csv pt.tl, ',', p_base.tl, ',', w.tl, ',', c.tl, ',';
 823      put f_composicao_csv p_rendimentoPadrao(pt,p_base,w,c):0:2 /;
 824  );
 825  putclose f_composicao_csv;
 826   
 827  * --- Relatorio 2: Plano de Producao (com saida inteira)
 828  put f_plano_csv 'DataProducao,Maquina,PadraoCorte,QtdBobinasMae' /;
 829  loop((pt,j,t)$(v_usaPadrao.l(pt,j,t) > 0.001),
 830      put f_plano_csv t.tl, ',', j.tl, ',', pt.tl, ',';
 831      put f_plano_csv v_usaPadrao.l(pt,j,t):0:0 /;
 832  );
 833  putclose f_plano_csv;
 834   
 835  * --- Relatorio 3: Status das Entregas vs. Demanda
 836  Parameter
 837      p_prodPorData(p_base,w,c,t)
 838      p_cumProd(p_base,w,c,t)
 839      p_cumDem(p_base,w,c,t)
 840      p_dataAtende_ord(p_base,w,c,t)
 841      p_diasDesvio(p_base,w,c,t);
 842  p_prodPorData(p_base,w,c,t) = sum((pt,j), p_rendimentoPadrao(pt,p_base,w,c) * v_usaPadrao.l(pt,j,t));
 843  p_cumProd(p_base,w,c,t) = sum(tt$(ord(tt) <= ord(t)), p_prodPorData(p_base,w,c,tt));
 844  p_cumDem(p_base,w,c,t)  = sum(tt$(ord(tt) <= ord(t)), p_demanda(p_base,w,c,tt));
 845  p_dataAtende_ord(p_base,w,c,t) = 0;
 846  loop((p_base,w,c,t)$(p_demanda(p_base,w,c,t) > 0),
 847      loop(t2$(p_cumProd(p_base,w,c,t2) >= p_cumDem(p_base,w,c,t) and p_dataAtende_ord(p_base,w,c,t) = 0),
 848          p_dataAtende_ord(p_base,w,c,t) = ord(t2);
 849      );
 850  );
 851  p_diasDesvio(p_base,w,c,t)$(p_dataAtende_ord(p_base,w,c,t) > 0) = p_dataAtende_ord(p_base,w,c,t) - ord(t);
 852   
 853  put f_status_csv 'PN_Base,LarguraProduto,CompProduto,DataEntregaRequerida,QtdDemandada,DataProducaoReal,DiasDesvio,StatusEntrega' /;
 854  loop((p_base,w,c,t)$(p_demanda(p_base,w,c,t) > 0),
 855      put f_status_csv p_base.tl, ',', w.tl, ',', c.tl, ',', t.tl, ',';
 856      put f_status_csv p_demanda(p_base,w,c,t):0:2, ',';
 857   
 858      if(p_dataAtende_ord(p_base,w,c,t) > 0,
 859          loop(t_prod$(ord(t_prod) = p_dataAtende_ord(p_base,w,c,t)),
 860              put f_status_csv t_prod.tl;
 861          );
 862          put f_status_csv ',', p_diasDesvio(p_base,w,c,t):0:0, ',';
 863          if (p_diasDesvio(p_base,w,c,t) > 0, put '1'; else put '2';);
 864      else
